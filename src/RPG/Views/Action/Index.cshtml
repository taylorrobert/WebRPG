@model RPG.Models.ActionModel
@{
    ViewData["Title"] = "Home Page";
}

<script>
    var model = @Json.Serialize(Model);
    var clientModel = model.ClientModel;
    $(document).ready(initView);
    $("#btnEndTurn").click(function() { endTurn('@Url.Action("EndTurn")'); });
</script>




